platform :ios, '11.0'

#inhibit_all_warnings! #关闭CocoaPods中的警告

target 'TheWeather' do
  
pod 'AFNetworking'                  #网络加载
pod 'LEEAlert'                      #警告框
pod 'Masonry'                       #自适应布局
pod 'IQKeyboardManager'             #键盘
pod 'MJExtension'                   #JSON
pod 'SDWebImage'                    #图片加载
pod 'FDFullscreenPopGesture'        #侧滑功能
#pod 'SDCycleScrollView'             #轮播图-------------->SDWebImage
pod 'CWLateralSlide'                #侧滑控件
pod 'FMDB'                          #数据库

end

